<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="text1" parent="TextAppearance.AppCompat.Widget.ActionBar.Title">
        <item name="android:textSize">@dimen/sp_14</item>
        <item name="android:textColor">#3087ea</item>
    </style>
    <style name="edtext1" parent="TextAppearance.AppCompat.Widget.ActionBar.Title">
        <item name="android:layout_width">@dimen/dp_300</item>
        <item name="android:layout_height">@dimen/dp_40</item>
        <item name="android:layout_marginTop">@dimen/dp_10</item>
        <item name="android:layout_marginLeft">@dimen/dp_30</item>
        <item name="android:background">@drawable/edit</item>
    </style>
    <style name="prabntext" parent="TextAppearance.AppCompat.Widget.ActionBar.Title">
        <item name="android:textSize">@dimen/sp_14</item>
        <item name="android:textColor">#333333</item>

    </style>
    <style name="prabntext1" parent="TextAppearance.AppCompat.Widget.ActionBar.Title">
        <item name="android:textSize">@dimen/sp_12</item>
        <item name="android:textColor">#666666</item>
        <item name="android:layout_marginRight">@dimen/dp_20</item>
        <item name="android:layout_alignParentRight">true</item>
    </style>
    <style name="MyEditText" parent="Theme.AppCompat.Light">
        <item name="colorControlNormal">#f2f2f2</item>
        <item name="colorControlActivated">#BBBB</item>
    </style>

    <style name="AppTheme.TransparentStausBar" parent="AppTheme">

    </style>
</resources>
